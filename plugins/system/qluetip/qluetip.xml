<?xml version="1.0" encoding="utf-8"?><extension version="1.6" type="plugin" group="system">  <name>Qlue ToolTip</name><creationDate>23rd March 2010</creationDate><author>Aaron Harding - Qlue</author><authorUrl>http://www.qlue.co.uk</authorUrl><license>GNU/GPL</license><version>1.6.3</version><description>PLG_TOOLTIP_DESCRIPTION</description><files>   <filename plugin="qluetip">qluetip.php</filename>   <folder>qluetip</folder></files><languages>   <language tag="en-GB">en-GB.plg_system_qluetip.ini</language>   <language tag="en-GB">en-GB.plg_system_qluetip.sys.ini</language></languages><config>  <fields name="params">     <fieldset name="basic">      <field type="radio" name="event" default="mouseenter" label="EVENT_LABEL" description="EVENT_DESCRIPTION">      	<option value="click" >EVENT_OPTION_1</option>      	<option value="mouseenter" >EVENT_OPTION_2</option>      </field>      <field type="radio" name="sticky" default="0" label="STICKY_LABEL" description="STICKY_DESCRIPTION">      	<option value="1" >STICKY_OPTION_1</option>      	<option value="0" >STICKY_OPTION_2</option>      </field>      <field type="radio" name="fixed" default="0" label="FIXED_LABEL" description="FIXED_DESCRIPTION" >      	<option value="1" >FIXED_OPTION_1</option>      	<option value="0" >FIXED_OPTION_2</option>      </field>      <field type="radio" name="position" default="item" label="POSITION_LABEL" description="POSITION_DESCRIPTION">        <option value="cursor" >POSITION_OPTION_1</option>        <option value="item" >POSITION_OPTION_2</option>      </field>      <field type="text" name="offsets" default="10,-5" label="OFFSETS_LABEL" description="OFFSETS_DESCRIPTION" />      <field type="text" name="width" default="350" label="WIDTH_LABEL" description="WIDTH_DESCRIPTION" />      <field type="text" name="duration" default="300" label="DURATION_LABEL" description="DURATION_DESCRIPTION" />      <field type="text" name="class" default="" label="CUSTOM_CLASS_LABEL" description="CUSTOM_CLASS_DESCRIPTION" />      <field type="radio" name="element" default="div" label="ELEMENT_LABEL" description="ELEMENT_DESCRIPTION" >      	<option value="div" >Div</option>    	  <option value="span" >Span</option>
      </field>     </fieldset>  </fields>
</config>
</extension>